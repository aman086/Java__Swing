import javax.swing.*;
import java.awt.*;

public class Main {
    public static void main(String[] args) {

        DemoWindow obj = new DemoWindow();

    }
}

class DemoWindow extends JFrame{


    DemoWindow(){

        setLayout(new FlowLayout());
        JLabel jl = new JLabel("Mai text hu");
        add(jl);

        JLabel jl2 = new JLabel("Mai bhi hu");
        add(jl2);



        setVisible(true);
        setSize(400 , 600);

        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
    }
}
